{
  "$schema": "https://tauri.app/v2/api/config",
  "productName": "逻辑拼图",
  "version": "0.1.0",
  "identifier": "com.logicpuzzle.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": false
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "逻辑拼图",
        "width": 1024,
        "height": 768,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}
